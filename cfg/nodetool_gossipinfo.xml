<?xml version="1.0"?>
<input>
    <!--Node label-->
    <line match="^(.+)?/(.+)">
        <part colour="default" style="bold" append=": ">2</part>
    </line>

    <line match="^\s+(SEVERITY:)([0-1]+\.[0-9]+\n)">
        <part colour="default" style="dim">1</part>
        <part colour="green">2</part>
    </line>
    <line match="^\s+(SEVERITY:)([2-9]+\.[0-9]+\n)">
        <part colour="default" style="dim">1</part>
        <part colour="yellow">2</part>
    </line>
</input>
